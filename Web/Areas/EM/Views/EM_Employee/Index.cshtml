}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EMUserName)
        </th>
      
        <th>
            @Html.DisplayNameFor(model => model.EMRealName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmType)
